    <trace autoflush="true">
      <listeners>
        <add name="textWriterTraceListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="C:\\temp\\SmartDbTrace.txt" />
      </listeners>
    </trace>
  </system.diagnostics>-->
  
</configuration>